{
	"$schema": "https://json.schemastore.org/package",
	"dependencies": {
		"@types/node": "^14.14.35"
	},
	"devDependencies": {
		"@types/yargs": "^16.0.0",
		"pkg": "^4.5.0",
		"prettier": "2.2.1",
		"typescript": "^4.2.3"
	},
	"name": "@norathefurry/elevate",
	"version": "1.0.4",
	"description": "Elevate whatever you want using `elevate [command]`",
	"main": "js/Elevate.js",
	"author": "0J3 <ElevateJS@wafflepowered.com>",
	"license": "LGPL-3.0-or-later",
	"repository": "https://github.com/0J3/Elevate.git",
	"scripts": {
		"test": "node ./js/Test.js",
		"start": "node ./js/Elevate.js",
		"build": "tsc --build ./tsconfig.json",
		"package": "pkg . -t node14-win --output bin/elevate.exe"
	},
	"bin": "./js/Elevate.js",
	"pkg": {
		"scripts": "js/*.js",
		"assets": "ps/*"
	}
}
